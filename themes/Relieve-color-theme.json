		// "statusBarItem.remoteBackground": "#4d78cc",
		// "statusBarItem.remoteForeground": "#f8fafd",
	},
	"tokenColors": [
			{
				"name": "Comments",
				"scope": [
					"comment",
					"punctuation.definition.comment"
				],
				"settings": {
					"foreground": "#E5C07B"
				}
			},
		{
			"name": "Meta",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "String, Regular Expressions and Symbols",
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "Keywords",
			"scope": "entity.name",
			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"name": "Import",
			"scope": [
				"variable.other.readwrite.alias",
			],
			"settings": {
				"foreground": "#9fc490"
			},
		},
				{
			"name": "Variable names",
			"scope": "variable.other.constant",
			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#660000",
				"background": "#96000014"
			}
		},
		{
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343",
				"background": "#DDDDFF"
			}
		},
		{
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343",
				"background": "#FFDDDD"
			}
		},
		{
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343",
				"background": "#DDFFDD"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		},
