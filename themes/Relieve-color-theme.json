	"tokenColors": [
			{
				"name": "Comments",
				"scope": [
					"comment",
					"punctuation.definition.comment"
				],
				"settings": {
					"foreground": "#E5C07B"
				}
			},
		{
			"name": "Meta",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "String, Regular Expressions and Symbols",
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"entity.name",
				"entity.name.function",
				"support.function.any-method",
				"entity.name.function.ts",
				"variable.function",
				"support.function.console",
				"keyword.other.special-method",
			],
			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"name": "Import",
			"scope": [
				"variable.other.readwrite.alias",
			],
			"settings": {
				"foreground": "#9fc490"
			},
		},
		{
			"name": "Variable names",
			"scope": [
				"variable.other.constant",
				"meta.definition.variable",
			],
			"settings": {
				"foreground": "#A2CBFA"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#8A97A1"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#660055",
			}
		},
		{
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#65737E",
			}
		},
		{
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#65737E",
			}
		},
		{
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343",
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css",
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#A2CBFA"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"name": "JSON Key - Value",
			"scope": [
				"meta.structure.dictionary.value.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"name": "JSON Key - Level 1",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#A2CBFA"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A2CBFA"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#A2CBFA"
			}
		},
		{
			"name": "Markup - Quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
